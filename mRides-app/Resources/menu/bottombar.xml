<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

  
     
 <!--Layout where the menu bottom tool bar is placed containing: My rides, My profile, Map, Chat, Settings-->
 
  <LinearLayout
   android:orientation="horizontal"
    android:paddingLeft="0dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="5.5dp"
    android:padding="10dp"
    android:layout_marginTop="0dp"
    android:alpha="0.78"
    android:id="@+id/matchingLinearLayout6">
    <FrameLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="77.5dp"
        android:layout_height="56.5dp"
        android:id="@+id/frameLayout2"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp">
      <Button
          android:id="@+id/menu_calendar"
          android:background="@drawable/calendar"
          android:layout_width="25dp"
          android:layout_height="25dp"
          android:layout_gravity="center"
          android:layout_marginBottom="18.5dp"
          android:layout_marginTop="4.0dp" />
      <TextView
          android:text="@string/menu_my_rides"
          android:textColor="#0A0A09"
          android:layout_width="match_parent"
          android:layout_height="45dp"
          android:id="@+id/textView2"
          android:layout_marginTop="30.0dp"
          android:textSize="12dp"
          android:layout_marginLeft="24.0dp"
          android:layout_marginBottom="13.0dp" />
    </FrameLayout>
    <FrameLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="52.5dp"
        android:layout_height="66.0dp"
        android:id="@+id/frameLayout1"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp">
      <Button
          android:layout_width="25dp"
          android:layout_height="25dp"
          android:id="@+id/menu_user"
          android:background="@drawable/user"
          android:layout_gravity="center"
          android:layout_marginBottom="20dp"
          android:layout_marginTop="0dp"
          android:layout_marginRight="6.5dp"
          android:layout_marginLeft="3.0dp" />
      <TextView
          android:text="@string/menu_my_profile"
          android:textColor="#0A0A09"
          android:layout_width="match_parent"
          android:layout_height="45dp"
          android:id="@+id/textView1"
          android:layout_marginTop="30.0dp"
          android:textSize="12dp"
          android:layout_marginLeft="5.5dp"
          android:layout_marginRight="2.5dp" />
    </FrameLayout>
    <FrameLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="56.5dp"
        android:layout_height="54.0dp"
        android:id="@+id/frameLayout3"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp">
      <Button
          android:layout_width="25dp"
          android:layout_height="25dp"
          android:id="@+id/menu_map"
          android:background="@drawable/house_menu"
          android:layout_gravity="center"
          android:layout_marginBottom="20dp"
          android:layout_marginTop="5.5dp"
          android:layout_marginRight="9.5dp"
          android:layout_marginLeft="9.0dp" />
      <TextView
          android:text="@string/menu_general"
          android:textColor="#0A0A09"
          android:layout_width="match_parent"
          android:layout_height="45dp"
          android:id="@+id/textView3"
          android:layout_marginTop="30.0dp"
          android:textSize="12dp"
          android:layout_marginLeft="12.0dp" />
    </FrameLayout>
    <FrameLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="72.0dp"
        android:layout_height="51.5dp"
        android:id="@+id/frameLayout4"
        android:layout_marginLeft="0.5dp"
        android:layout_marginRight="0dp">
      <Button
          android:layout_width="25dp"
          android:layout_height="25dp"
          android:id="@+id/menu_chat"
          android:background="@drawable/chat"
          android:layout_gravity="center"
          android:layout_marginBottom="20dp"
          android:layout_marginTop="7.0dp"
          android:layout_marginRight="3.0dp" />
      <TextView
          android:text="@string/menu_chat"
          android:textColor="#0A0A09"
          android:layout_width="match_parent"
          android:layout_height="45dp"
          android:id="@+id/textView4"
          android:layout_marginTop="30.0dp"
          android:textSize="12dp"
          android:layout_marginLeft="5.5dp" />
    </FrameLayout>
    <FrameLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="72.5dp"
        android:layout_height="64.0dp"
        android:id="@+id/frameLayout5"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="23.5dp">
      <Button
          android:layout_width="25dp"
          android:layout_height="25dp"
          android:id="@+id/menu_settings"
          android:background="@drawable/settings"
          android:layout_gravity="center"
          android:layout_marginBottom="20dp"
          android:layout_marginTop="2.0dp"
          android:layout_marginRight="13.0dp"
          android:layout_marginLeft="0dp" />
      <TextView
          android:text="@string/menu_settings"
          android:textColor="#0A0A09"
          android:layout_width="match_parent"
          android:layout_height="45dp"
          android:id="@+id/textView5"
          android:layout_marginTop="30.0dp"
          android:textSize="12dp"
          android:layout_marginLeft="2.5dp"
          android:layout_marginRight="0dp" />
    </FrameLayout>
  </LinearLayout>






</merge>
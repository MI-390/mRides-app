<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#73ad53" />
      <stroke
          android:width="1dp"
          android:color="#92d36e" />
      <corners
          android:radius="6dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#92d36e"
          android:endColor="#73ad53"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#92d36e" />
      <corners
          android:radius="6dp" />
    </shape>
  </item>
</selector>